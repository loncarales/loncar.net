[localhost]
127.0.0.1 ansible_connection=local ansible_python_interpreter=/path/to/ansible_python_interpreter

[loncar_net]
192.168.56.100 ansible_user=vagrant ansible_python_interpreter=/usr/bin/python3 ansible_ssh_private_key_file=/path/to/vagrant_ssh_private_key

[hugo]
<SERVER-IP> ansible_python_interpreter=/usr/bin/python3

[all:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
